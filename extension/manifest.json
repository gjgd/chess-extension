{
  "manifest_version": 2,
  "name": "Chess extension",
  "version": "1.0",
  "description": "Disables the option to play fast time formats on chess.com",
  "icons": {
    "32": "icons/chess-32.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.chess.com/*"],
      "js": ["content_script.js"]
    }
  ]
}
